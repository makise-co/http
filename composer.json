{
    "name": "makise-co/http",
    "description": "Makise-Co HTTP server",
    "keywords": [
        "micro",
        "framework",
        "makise",
        "coroutine",
        "swoole",
        "http",
        "server"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry K.",
            "email": "coder1994@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "suggest": {
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-swoole": "^4.4",
        "filp/whoops": "^2.9",
        "laminas/laminas-diactoros": "^2.4",
        "makise-co/framework": "~2.0.0",
        "makise-co/http-router": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.1",
        "willdurand/negotiation": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^9.0",
        "swoole/ide-helper": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "MakiseCo\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakiseCo\\Http\\Tests\\": "tests/"
        }
    }
}
